require 'rubygems'

version = ">= 0"

if ARGV.first =~ /^_(.*)_$/ and Gem::Version.correct? $1 then
  version = $1
  ARGV.shift
end

gem 'ruby2ruby', version
load Gem.bin_path('ruby2ruby', 'r2r_show', version)
